<?xml version="1.0" encoding="utf-8"?><!-- Represents a crossed line indicating that the daily goal set for a date
     was not met -->
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="-47">

    <inset
        android:insetBottom="@dimen/calendar_view_inset"
        android:insetLeft="@dimen/calendar_view_inset"
        android:insetRight="@dimen/calendar_view_inset"
        android:insetTop="@dimen/calendar_view_inset">

        <shape android:shape="line">
            <stroke
                android:width="1.5dp"
                android:color="@color/black30" />
        </shape>
    </inset>
</rotate>